<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/createpage"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/createaccount"
    android:elevation="4dp">

    <ImageButton
        android:id="@+id/createback"
        android:layout_width="57dp"
        android:layout_height="59dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/createback"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/createidlabel"
        android:layout_width="356dp"
        android:layout_height="93dp"
        android:layout_marginTop="172dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.487"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/createback"
        app:srcCompat="@drawable/createidlabel" />

    <EditText
        android:id="@+id/userId"
        android:layout_width="207dp"
        android:layout_height="42dp"
        android:layout_marginTop="28dp"
        android:maxLines="1"
        android:hint="ID"
        app:layout_constraintEnd_toEndOf="@id/createidlabel"
        app:layout_constraintHorizontal_bias="0.134"
        app:layout_constraintStart_toStartOf="@id/createidlabel"
        app:layout_constraintTop_toTopOf="@id/createidlabel" />

    <ImageButton
        android:id="@+id/btn_id_check"
        android:layout_width="57dp"
        android:layout_height="39dp"
        android:layout_marginTop="284dp"
        android:background="@drawable/jungbok"
        app:layout_constraintEnd_toEndOf="@id/createidlabel"
        app:layout_constraintHorizontal_bias="0.759"
        app:layout_constraintStart_toEndOf="@id/userId"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/createpwlabel"
        android:layout_width="362dp"
        android:layout_height="67dp"
        android:layout_marginTop="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.457"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/createidlabel"
        app:srcCompat="@drawable/createpwlabel" />

    <EditText
        android:id="@+id/user_password"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:inputType="textPassword"
        android:maxLines="1"
        android:hint="Password"
        app:layout_constraintEnd_toEndOf="@id/createpwlabel"
        app:layout_constraintHorizontal_bias="0.157"
        app:layout_constraintStart_toStartOf="@id/createpwlabel"
        app:layout_constraintTop_toTopOf="@id/createpwlabel" />

    <ImageView
        android:id="@+id/createcflabel"
        android:layout_width="355dp"
        android:layout_height="76dp"
        android:layout_marginTop="28dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.38"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/createpwlabel"
        app:srcCompat="@drawable/createcflabel" />

    <EditText
        android:id="@+id/user_password_again"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:ems="10"
        android:inputType="textPassword"
        android:maxLines="1"
        android:hint="Password"
        app:layout_constraintEnd_toEndOf="@id/createcflabel"
        app:layout_constraintHorizontal_bias="0.165"
        app:layout_constraintStart_toStartOf="@id/createcflabel"
        app:layout_constraintTop_toTopOf="@id/createcflabel" />

    <ImageButton
        android:id="@+id/btn_next"
        android:layout_width="175dp"
        android:layout_height="70dp"
        android:layout_marginTop="60dp"
        android:background="@drawable/createcontinue"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.905"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/createcflabel" />

</androidx.constraintlayout.widget.ConstraintLayout>
